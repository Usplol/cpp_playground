{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Make File",
			"command": "C:\\MinGW\\bin\\mingw32-make.exe",
			"args": [
				"all",
				"-j16"
			],
			"options": {
				"cwd": "${workspaceRoot}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"type": "cppbuild",
			"label": "C/C++: g++.exe build active file",
			"command": "C:\\MinGW\\bin\\g++.exe",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"-Wall",
				"-Weffc++",
				"-Wextra",
				"-Wsign-conversion",
				"-Werror",
				"--std=c++17",
				"${file}",
				"-o",
				"${fileDirname}\\${fileBasenameNoExtension}.exe"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "Task generated by Debugger."
		}
	]
}